#:kivy 2.3.0
<TestDashboard>:
    BoxLayout:
        orientation: 'vertical'
        # padding: 10
        spacing: 10
        BoxLayout:
            orientation: 'vertical'
            padding: 20
            size_hint: 1, 0.2
            Button:
                size_hint: None, None
                height: 40
                width: 40
                background_normal: ''
                background_color: 1, 1, 1, 1
                # padding_x: 50
                # pos_hint: {'center_x':.1, 'center_y':.9}
                on_press: root.logout_popup()
                Image:
                    source: 'asset/logout.png'
                    size_hint: None, None
                    size: self.parent.width * 1.0, self.parent.height * 1.0
                    pos: self.parent.center_x - self.width / 2, self.parent.center_y - self.height / 2
            Label:
                text:"Dashboard"
                font_size: "40dp"
                halign:"center"
                # pos_hint: {'center_y':.87}
                color:"#8366DD"
        BoxLayout:
            orientation:'vertical'
            size_hint: 0.9, 0.9
            halign:'center'
            pos_hint:{'center_x':.5}
            padding: 10
            spacing: 75
            GridLayout:
                id: container
                cols: 2
                spacing: 20
                size_hint_y: None
                height: self.minimum_height
                Button:
                    size_hint: 0.5, None
                    height: 200
                    # width: 133
                    background_normal: ''
                    background_color: "#D2BBFF"
                    # padding_x: 50
                    # pos_hint: {'center_x':.5, 'center_y':.8}
                    on_press: 
                        app.root.current = 'pemilihanrw'
                    BoxLayout:
                        orientation: 'vertical'  # Mengatur orientasi menjadi vertikal
                        size_hint: None, None
                        pos: self.parent.center_x - self.width / 2, self.parent.center_y - self.height / 2
                        Image:
                            source: 'asset/manager.png'
                            size_hint: None, None
                            width: 80
                            height: 80
                            pos_hint: {'center_x': .5}  # Pusatkan gambar secara horizontal
                        Label:
                            text: "Pemilihan RW"
                            font_size: "12dp"
                            halign: "center"
                            color: "#000000"
                            size_hint_y: None  # Ukuran Y diatur manual
                            height: 30  # Tinggi Label
                            valign: 'middle'  # Pusatkan teks vertikal
                            text_size: self.size  # Mengatur teks agar sesuai dengan ukuran Label
                
                Button:
                    size_hint: 0.5, None
                    height: 200
                    # width: 133
                    background_normal: ''
                    background_color: "#D2BBFF"
                    # padding_x: 50
                    # pos_hint: {'center_x':.5, 'center_y':.8}
                    on_press: 
                        app.root.current = 'pemilihanrt'
                    BoxLayout:
                        orientation: 'vertical'  # Mengatur orientasi menjadi vertikal
                        size_hint: None, None
                        pos: self.parent.center_x - self.width / 2, self.parent.center_y - self.height / 2
                        Image:
                            source: 'asset/manager.png'
                            size_hint: None, None
                            width: 80
                            height: 80
                            pos_hint: {'center_x': .5}  # Pusatkan gambar secara horizontal
                        Label:
                            text: "Pemilihan RT"
                            font_size: "12dp"
                            halign: "center"
                            color: "#000000"
                            size_hint_y: None  # Ukuran Y diatur manual
                            height: 30  # Tinggi Label
                            valign: 'middle'  # Pusatkan teks vertikal
                            text_size: self.size  # Mengatur teks agar sesuai dengan ukuran Label
        
                Button:
                    size_hint: 0.5, None
                    height: 200
                    # width: 133
                    background_normal: ''
                    background_color: "#D2BBFF"
                    # padding_x: 50
                    # pos_hint: {'center_x':.5, 'center_y':.5}
                    on_press: 
                        app.root.current = 'hasil'
                    BoxLayout:
                        orientation: 'vertical'  # Mengatur orientasi menjadi vertikal
                        size_hint: None, None
                        pos: self.parent.center_x - self.width / 2, self.parent.center_y - self.height / 2
                        Image:
                            source: 'asset/gambardiagram.png'
                            size_hint: None, None
                            width: 80
                            height: 80
                            pos_hint: {'center_x': .5}  # Pusatkan gambar secara horizontal
                        Label:
                            text: "Hasil"
                            font_size: "12dp"
                            halign: "center"
                            color: "#000000"
                            size_hint_y: None  # Ukuran Y diatur manual
                            height: 30  # Tinggi Label
                            valign: 'middle'  # Pusatkan teks vertikal
                            text_size: self.size  # Mengatur teks agar sesuai dengan ukuran Label
                Button:
                    size_hint: 0.5, None
                    height: 200
                    # width: 133
                    background_normal: ''
                    background_color: "#D2BBFF"
                    # padding_x: 50
                    # pos_hint: {'center_x':.5, 'center_y':.5}
                    on_press: 
                        app.root.current = 'profile'
                    BoxLayout:
                        orientation: 'vertical'  # Mengatur orientasi menjadi vertikal
                        size_hint: None, None
                        pos: self.parent.center_x - self.width / 2, self.parent.center_y - self.height / 2
                        Image:
                            source: 'asset/profile-user.png'
                            size_hint: None, None
                            width: 80
                            height: 80
                            pos_hint: {'center_x': .5}  # Pusatkan gambar secara horizontal
                        Label:
                            text: "Profile"
                            font_size: "12dp"
                            halign: "center"
                            color: "#000000"
                            size_hint_y: None  # Ukuran Y diatur manual
                            height: 30  # Tinggi Label
                            valign: 'middle'  # Pusatkan teks vertikal
                            text_size: self.size  # Mengatur teks agar sesuai dengan ukuran Label

            BoxLayout:
                orientation: 'vertical'
                size_hint: 1, 0.2